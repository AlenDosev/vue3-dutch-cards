{
  "name": "vue3-dutch-cards",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview --port 5050",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "deploy": "sh deploy.sh",
    "format": "prettier --check \"**/*.{js,json,vue,scss}\" --write"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "pinia": "^2.2.2",
    "sass": "^1.79.3",
    "vue": "^3.5.8",
    "vue-axios": "^3.5.2",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.10.4",
    "@types/node": "^22.6.1",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.5.0",
    "eslint-plugin-vue": "^8.2.0",
    "prettier": "^3.3.3",
    "typescript": "5.6.2",
    "vite": "^5.4.7",
    "vue-tsc": "^2.1.6"
  }
}